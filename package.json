{
    "name": "@oudshoorn-hydraulics/exact-globe-soap-client",
    "version": "2.0.11",
    "description": "A typescript/javascript soap client for Exact Globe",
    "keywords": [
        "Exact globe",
        "entity services",
        "soap"
    ],
    "author": "Mathijs van de Venne",
    "main": "dist/index.cjs",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "require": "./dist/index.cjs",
            "import": "./dist/index.js"
        }
    },
    "type": "module",
    "repository": {
        "type": "git",
        "url": "https://github.com/oudshoorn-hydraulics/exact-globe-soap-client.git"
    },
    "scripts": {
        "compile": "tsup",
        "build": "rm -rf ./dist && npm run compile",
        "lint": "npx eslint .",
        "test:integration": "vitest --project=integration run",
        "test:unit": "vitest --project=unit run"
    },
    "dependencies": {
        "@xmldom/xmldom": "^0.8.6",
        "effect": "^3.19.13",
        "soap": "^1.0.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.26.0",
        "@types/node": "^22.10.3",
        "@types/sax": "^1.2.4",
        "dotenv": "^16.5.0",
        "eslint": "^9.26.0",
        "eslint-config-prettier": "^10.1.2",
        "prettier": "^3.4.2",
        "prettier-plugin-organize-imports": "^4.3.0",
        "tsup": "^8.5.1",
        "typescript": "^5.3.3",
        "typescript-eslint": "^8.31.1",
        "vitest": "^3.1.2"
    },
    "engines": {
        "node": ">=24.0.0 <25.0.0"
    }
}
